[sub_resource type="GDScript" id="GDScript_rhbkm"]
script/source = "extends GridContainer

func _ready():
	await get_tree().process_frame
	_setup_grid_slots()

func _setup_grid_slots():
	# ลำดับชั้น: Grid -> Scroll -> SaveSlot(Node2D)
	var main_node = get_parent().get_parent() 
	
	for child in get_children():
		if child is Button:
			# สร้างชื่อฟังก์ชันเป้าหมาย เช่น \"_on_saveslot_2_pressed\"
			var func_name = \"_on_\" + child.name + \"_pressed\"
			
			# ตรวจสอบว่าใน Node2D มีฟังก์ชันนี้อยู่จริงไหม
			if main_node.has_method(func_name):
				if not child.pressed.is_connected(Callable(main_node, func_name)):
					child.pressed.connect(Callable(main_node, func_name))
			
			if main_node.has_method(\"_play_hover\"):
				child.mouse_entered.connect(main_node._play_hover)

			_disable_children_mouse(child)

func _disable_children_mouse(node: Node):
	for child in node.get_children():
		if child is Control:
			child.mouse_filter = Control.MOUSE_FILTER_IGNORE
		_disable_children_mouse(child)
"

